<style>
    .u-hidden{
        display:none;
    }

    code{
        font-familly:monospace;
        font-size:11px;
        color:#666;
    }

    .wrap{
        display: flex;
        flex-wrap: wrap;
    }

    div.item{
        position:relative;
        display: inline-block;
        margin:10px;
        width: 120px;
        height: 100px;
        text-align:center;
    }
    div.item>svg {
        display: inline-block;
        border: 1px solid #eee;
        width: 60px;
        height: 60px;
        margin-bottom:10px;
    }
</style>

{{{system}}}
<div class="wrap">
{{#each symbols}}
    <div class="item">
        <svg>
            <use xlink:href="#{{name}}"></use>
        </svg>
        <code>#{{name}}</code>
    </div>
{{/each}}
</div>
